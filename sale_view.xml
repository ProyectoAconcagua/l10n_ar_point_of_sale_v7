<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <!--vista Heredada de Sale_Shop-->
        <!--para agregar los shops_ids que contiene esa sucurzal-->
        <record id="view_shop_form1" model="ir.ui.view">
            <field name="name">sale.view.shop.form.inherit1</field>
            <field name="model">sale.shop</field>
            <field name="type">form</field>
            <field name="inherit_id" ref="sale.view_shop_form"/>
            <field name="arch" type="xml">
                <field name="project_id" position="after">
                    <separator string="" colspan="4"/>
                    <field name="pos_ar_ids" colspan="4" nolabel="1">
                        <tree string="Points of Sales">
                            <field name="name" />
                            <field name="shop_id" />
                            <field name="priority" />
                        </tree>
                    </field>
                </field>
            </field>
        </record>
    </data>
</openerp>
